@model HomeView

<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1">

@foreach (var metaTag in Model.MetaTags)
{
    <meta name="@metaTag.Name" content="@Html.Raw(metaTag.Content)" />
}


<title>Jarosław Armatys - Profesjonalne strony internetowe i aplikacje WWW - @ViewBag.Title</title>

<link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/cookieconsent@3/build/cookieconsent.min.css" />

<link rel="icon" href="~/user/images/favicon.png">